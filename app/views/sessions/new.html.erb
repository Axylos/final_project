<center><%= image_tag "logo.png", class: "main-img" %></center>

<content class="login">
  <h1>Returning User</h1>


  <form class="sign-up" action="<%= session_url %>" method="post">
    <input
      type="hidden"
      name="authenticity_token"
      value="<%= form_authenticity_token %>">

    <label for "email">Email</label>
    <input type="email" name="user[email]" class="log-inputs">

    <label for "password">Password</label>
    <input type="password" name="user[password]" class="log-inputs">

    <input type="submit" value="Sign In!">

  </form>
</content>
